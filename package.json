{
  "name": "nextjs",
  "packageManager": "yarn@4.6.0",
  "workspaces": [
    "server",
    "client"
  ],
  "scripts": {
    "upg": "yarn upgrade-interactive",
    "build": "turbo run build --concurrency=3",
    "dev": "turbo run dev --concurrency=3",
    "dc:build": "docker compose build",
    "dc:up": "docker compose up -d",
    "dc:start": "docker compose start",
    "dc:stop": "docker compose stop",
    "dc:rm": "docker compose rm"
  },
  "devDependencies": {
    "turbo": "^2.5.3"
  }
}
